{
  "name": "timeline",
  "version": "1.0.0",
  "description": "Microsite to display events compiled by Lawyers for Good Government Volunteers.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint '**/*.js'",
    "test": "rimraf dist && NODE_ENV=test webpack --hide-modules",
    "test:watch": "rimraf dist && NODE_ENV=test TEST_TYPE=unit webpack --watch --hide-modules",
    "test:unit": "rimraf dist && NODE_ENV=test TEST_TYPE=unit webpack --hide-modules",
    "test:integration": "rimraf dist && NODE_ENV=test TEST_TYPE=integration webpack --hide-modules",
    "start": "NODE_ENV=development webpack-dev-server --progress --colors --watch",
    "prod": "node server",
    "build": "NODE_ENV=production webpack --display-error-details",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/L4GG/timeline.git"
  },
  "author": "Lawyers for Good Government <info@lawyersforgoodgovernment.org> (https://lawyersforgoodgovernment.org)",
  "license": "MIT",
